    <action protect="true" name="startAutoTour">

        set(autoTourRuns, true);
        nextSceneLoop(+1);
        delayedcall(autotour,5.0,startAutoTour(););

    </action>

    <action protect="true" name="stopAutoTour">

        stopdelayedcall(autotour);
        set(autoTourRuns, false);

