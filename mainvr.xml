        set(events.onkeydown, stoptween(view.tx|view.ty|view.tz);set(events.onkeydown,null); );
        tween(view.tx|view.ty|view.tz, calc('' + image.ox + '|' + image.oy + '|' + image.oz), 2, default, set(events.onkeydown,null); );
        
